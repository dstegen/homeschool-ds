{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
      "no-console": "off",
      "no-useless-escape": "off",
      "no-unused-vars": "off"
    },
    "overrides": [
      {
        "files": ["data/*"],
        "rules": {
          "no-unused-vars": "off"
        }
      }
    ]
}
